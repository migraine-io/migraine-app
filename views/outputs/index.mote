% outputs = params[:outputs]

<h2><a href="/outputs/new">New Output</a></h2>

<h2>Your Output Types record:</h2>
<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Duration</th>
      <th>Scale</th>
    </tr>
  </thead>
  <tbody>
    % outputs.each do |output|
      <tr>
        <td>{{output.date}}</td>
        <td>{{output.duration}}</td>
        <td>{{output.scale}}</td>
      </tr>
    % end
  </tbody>
</table>
